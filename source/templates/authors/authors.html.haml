---
title: Reviewed Cast and Crew
layout: :layout_columns
---
:ruby
  content_for(:title, 'Reviewed Cast and Crew')
  content_for(:inline_css, 'source/templates/authors/authors.scss')
  content_for(:javascript, javascript_include_tag('filters.js', async: :async))

  authors = Booklog.authors

- content_for :layout_column_1 do
  = partial('templates/authors/authors_header')
  = partial('templates/authors/authors_filters')

- content_for :layout_column_2 do
  .authors
    = partial('templates/authors/authors_list', locals: { authors: authors.values })

